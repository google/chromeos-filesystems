{
  "name": "Amazon S3 File System",
  "description": "Access the contents of an S3 bucket directly from the Files app.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "aws-sdk.js",
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icon/16.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "fileSystemProvider",
    "storage"
  ]
}
